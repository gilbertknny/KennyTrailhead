    <formulas>
        <name>formatTDP</name>
        <dataType>String</dataType>
        <expression>&quot;Rp. &quot; &amp; IF(
    LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) &gt; 15,
    LEFT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 15) &amp; &quot;.&quot; &amp;
    MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 14, 3) &amp; &quot;.&quot; &amp;
    MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 11, 3) &amp; &quot;.&quot; &amp;
    MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 8, 3) &amp; &quot;.&quot; &amp;
    MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 5, 3) &amp; &quot;.&quot; &amp;
    RIGHT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), 3),
    IF(
        LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) &gt; 12,
        LEFT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 12) &amp; &quot;.&quot; &amp;
        MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 11, 3) &amp; &quot;.&quot; &amp;
        MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 8, 3) &amp; &quot;.&quot; &amp;
        MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 5, 3) &amp; &quot;.&quot; &amp;
        RIGHT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), 3),
        IF(
            LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) &gt; 9,
            LEFT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 9) &amp; &quot;.&quot; &amp;
            MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 8, 3) &amp; &quot;.&quot; &amp;
            MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 5, 3) &amp; &quot;.&quot; &amp;
            RIGHT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), 3),
            IF(
                LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) &gt; 6,
                LEFT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 6) &amp; &quot;.&quot; &amp;
                MID(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 5, 3) &amp; &quot;.&quot; &amp;
                RIGHT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), 3),
                IF(
                    LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) &gt; 3,
                    LEFT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), LEN(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})) - 3) &amp; &quot;.&quot; &amp;
                    RIGHT(TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c}), 3),
                    TEXT({!Get_Loan_Calculation_Result.total_down_payment_amount__c})
                )
            )
        )
    )
)</expression>
