    <formulas>
        <name>Date_Of_Birth</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!Get_Lead.Date_of_Birth__c})) + &quot;-&quot; +
LPAD(TEXT(MONTH({!Get_Lead.Date_of_Birth__c})), 2, &quot;0&quot;) + &quot;-&quot; +
LPAD(TEXT(DAY({!Get_Lead.Date_of_Birth__c})), 2, &quot;0&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_Gender</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_Lead.Primary_Contact__r.GenderIdentity} )== &quot;Male&quot;, &quot;M&quot;, &quot;F&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_MaritalStatus</name>
        <dataType>String</dataType>
        <expression>IF({!Get_Lead.Primary_Contact__r.Marital_Status__r.Name} == &quot;Married&quot;, &quot;Kawin&quot;, 
IF({!Get_Lead.Primary_Contact__r.Marital_Status__r.Name} == &quot;Divorce&quot;, &quot;Cerai Hidup&quot;, 
IF({!Get_Lead.Primary_Contact__r.Marital_Status__r.Name} == &quot;Widow&quot;, &quot;Cerai Mati&quot;, &quot;Belum Kawin&quot;)))</expression>
    </formulas>
    <formulas>
        <name>Negative_List_Result_Formula</name>
        <dataType>String</dataType>
        <expression>LEFT({!Negative_List_Result}, (LEN( {!Negative_List_Result}) - 2))</expression>
    </formulas>
