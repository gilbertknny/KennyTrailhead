    <description>Fixing no birthdate in company
Fixing address for province
Set company address with legal address
Set residence address with legal address
repromote
expected the latest</description>
    <environments>Default</environments>
    <formulas>
        <name>funcS_birth_date</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!curGuarantor.Date_of_Birth_Guarantor__c})) &amp; &quot;-&quot; &amp;
LPAD(TEXT(MONTH({!curGuarantor.Date_of_Birth_Guarantor__c})),2,&quot;0&quot;) &amp; &quot;-&quot; &amp;
LPAD(TEXT(DAY({!curGuarantor.Date_of_Birth_Guarantor__c})),2,&quot;0&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_gender</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!curGuarantor.Gender__c})=&quot;Male&quot;, &quot;M&quot;,&quot;F&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_Guarantor_IndustryType</name>
        <dataType>String</dataType>
        <expression>TEXT({!curGuarantor.Industry_Type__r.id__c})</expression>
    </formulas>
    <formulas>
        <name>funcS_GuarantorType</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!curGuarantor.Guarantor_Type__c}, &quot;Personal Guarantor&quot;) == TRUE || ISPICKVAL({!curGuarantor.Guarantor_Type__c}, &quot;Payment Guarantor&quot;) == TRUE, &quot;INDIVIDUAL&quot;, &quot;INSTITUTION&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_IDType</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!curGuarantor.Guarantor_Type__c}, &quot;Personal Guarantor&quot;) == TRUE || ISPICKVAL({!curGuarantor.Guarantor_Type__c}, &quot;Payment Guarantor&quot;) == TRUE, &quot;KTP&quot;, &quot;NPWP&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcS_occupationId</name>
        <dataType>String</dataType>
        <expression>TEXT({!curGuarantor.Profession__r.id__c})</expression>
    </formulas>
