<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>65.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Create_Street</name>
        <label>Create Street</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>$Record.BillingStreet</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Address__c</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Location</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Check_Condition</name>
        <label>Check Condition</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Its_Insert</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>IsInsert</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Run_Duplicate</targetReference>
            </connector>
            <label>Its Insert</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>IsInsert</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()</expression>
    </formulas>
    <interviewLabel>[Account]Set Location {!$Flow.CurrentDateTime}</interviewLabel>
    <label>[Account]Set Location</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Location</name>
        <label>Update Location</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Check_Condition</targetReference>
        </connector>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Call_Flow</targetReference>
        </connector>
        <filterFormula>NOT {!$Permission.ByPassFlow} &amp;&amp; ISNULL({!$Record.Address_LU__c})</filterFormula>
        <object>Account</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Obsolete</status>
    <subflows>
        <name>Call_Flow</name>
        <label>Call Flow</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Create_Street</targetReference>
        </connector>
        <flowName>Location_Auto_Launched_map_Location_to_Standard_Location</flowName>
        <inputAssignments>
            <name>recordId</name>
            <value>
                <elementReference>$Record.Address_LU__c</elementReference>
            </value>
        </inputAssignments>
        <outputAssignments>
            <assignToReference>$Record.BillingCity</assignToReference>
            <name>CityName</name>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>$Record.BillingCountry</assignToReference>
            <name>CountryName</name>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>$Record.BillingState</assignToReference>
            <name>ProvinceName</name>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>$Record.BillingPostalCode</assignToReference>
            <name>ZipCodeName</name>
        </outputAssignments>
    </subflows>
    <subflows>
        <name>Run_Duplicate</name>
        <label>Run Duplicate</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <flowName>Account_Duplicate_Check</flowName>
        <inputAssignments>
            <name>AccountName</name>
            <value>
                <elementReference>$Record.Name</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>Address</name>
            <value>
                <elementReference>$Record.BillingStreet</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>Email</name>
            <value>
                <elementReference>$Record.Email__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>Phone</name>
            <value>
                <elementReference>$Record.Phone</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>ZipCode</name>
            <value>
                <elementReference>$Record.Zip__r.Zip_Code_Name__c</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
</Flow>
