    <formulas>
        <name>RemoveErrorComma</name>
        <dataType>String</dataType>
        <expression>IF(
    LEFT({!Error}, 1) = &quot;,&quot;, 
    IF(
        RIGHT(MID({!Error}, 2, LEN({!Error}) - 1), 1) = &quot;,&quot;, 
        LEFT(MID({!Error}, 2, LEN({!Error}) - 1), LEN(MID({!Error}, 2, LEN({!Error}) - 1)) - 1), 
        MID({!Error}, 2, LEN({!Error}) - 1)
    ), 
    IF(
        RIGHT({!Error}, 1) = &quot;,&quot;, 
        LEFT({!Error}, LEN({!Error}) - 1), 
        {!Error}
    )
)</expression>
    </formulas>
