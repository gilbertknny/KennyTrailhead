    <formulas>
        <name>func_CreateByNameFunding</name>
        <dataType>String</dataType>
        <expression>{!Get_User_CCO_Created_By.FirstName} &amp; &apos; &apos; &amp; {!Get_User_CCO_Created_By.LastName}</expression>
    </formulas>
    <formulas>
        <name>func_installment</name>
        <dataType>Number</dataType>
        <expression>IF({!Loop_Term_and_Condition_By_Step.Name} = &quot;Hold Dana Angsuran&quot;, 
{!Loop_Term_and_Condition_By_Step.TC_Value_Currency__c}/{!Get_Calculator.installment__c} ,0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>func_installmentfunding</name>
        <dataType>Number</dataType>
        <expression>IF({!Loop_Funding.Funding_Title__c} = &quot;Pocket - Angsuran&quot;, 
{!Loop_Funding.Funding_Value_Currency__c}/{!Get_Calculator.installment__c} ,0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>func_NotesFunding</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Loop_Funding.Keterangan__c}),&apos;-&apos;,{!Loop_Funding.Keterangan__c})</expression>
    </formulas>
    <formulas>
        <name>func_NotesTC</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Loop_Term_and_Condition_By_Step.Keterangan__c}),&apos;-&apos;,{!Loop_Term_and_Condition_By_Step.Keterangan__c})</expression>
    </formulas>
    <formulas>
        <name>func_ReviewerName</name>
        <dataType>String</dataType>
        <expression>{!Get_User_Created_By.FirstName} &amp; &apos; &apos; &amp; {!Get_User_Created_By.LastName}</expression>
    </formulas>
    <formulas>
        <name>func_TCFundingValue</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Loop_Funding.Funding_Value_Currency__c}),{!Loop_Funding.Funding_Value__c}, TEXT({!Loop_Funding.Funding_Value_Currency__c}))</expression>
    </formulas>
    <formulas>
        <name>func_TCName</name>
        <dataType>String</dataType>
        <expression>IF({!Loop_Term_and_Condition_By_Step.Name} = &quot;Hold Dana Angsuran&quot;, {!Loop_Term_and_Condition_By_Step.Name} + &quot; (&quot; + TEXT({!func_installment})  + &quot;x) &quot; , {!Loop_Term_and_Condition_By_Step.Name} )</expression>
    </formulas>
    <formulas>
        <name>func_TCNameFunding</name>
        <dataType>String</dataType>
        <expression>IF({!Loop_Funding.Funding_Name_Formula__c} = &quot;FUNDS_ON_HOLD_INSTALLMENTS&quot;, {!Loop_Funding.Funding_Title__c} + &quot; (&quot; + TEXT({!func_installmentfunding}) + &quot;x) &quot;, {!Loop_Funding.Funding_Title__c} )</expression>
    </formulas>
    <formulas>
        <name>func_TCValue</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Loop_Term_and_Condition_By_Step.TC_Value_Currency__c}), {!Loop_Term_and_Condition_By_Step.TC_Value__c}, TEXT({!Loop_Term_and_Condition_By_Step.TC_Value_Currency__c}))</expression>
    </formulas>
