    <formulas>
        <name>func_AdminFee</name>
        <dataType>Currency</dataType>
        <expression>IF(ISBLANK({!Get_Admin_Fee.admin_fee__c}), {!$Record.Admin_Fee__c}, {!Get_Admin_Fee.admin_fee__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>func_CustomerType</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!$Record.Opportunity__r.Customer__c}) == &quot;New&quot;, &quot;N&quot;, &quot;R&quot;)</expression>
    </formulas>
    <formulas>
        <name>func_EffectiveRate</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!Get_Effective_Rate.rate__c}), {!$Record.Effective_Rate__c}, {!Get_Effective_Rate.rate__c}*100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>func_ProvisionFee</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK({!Get_Provision_Fee.rate__c}), {!$Record.Provision_Fee__c}, {!Get_Provision_Fee.rate__c} * 100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>func_ProvisionFeeAmount</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.Opportunity__r.NTF_Pengajuan__c} * ({!func_ProvisionFee}/100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>varF_customer_type</name>
        <dataType>String</dataType>
        <expression>if({!$Record.Customer_Type__c} == &quot;New&quot;, &quot;N&quot;, &quot;R&quot;)</expression>
    </formulas>
