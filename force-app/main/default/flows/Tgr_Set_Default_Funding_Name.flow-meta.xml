    <formulas>
        <name>func_FundingValueText</name>
        <dataType>String</dataType>
        <expression>IF (OR(MID({!$Record.Funding_Value__c}, LEN({!$Record.Funding_Value__c}) - 2, 1) = &quot;,&quot; , MID({!$Record.Funding_Value__c}, LEN({!$Record.Funding_Value__c}) - 2, 1)  = &quot;.&quot;),
 SUBSTITUTE(LEFT({!$Record.Funding_Value__c},LEN({!$Record.Funding_Value__c})-3), &quot;,&quot;, &quot;.&quot;) + SUBSTITUTE(MID({!$Record.Funding_Value__c}, LEN({!$Record.Funding_Value__c}) - 2, 3), &quot;.&quot;, &quot;,&quot;)  ,
SUBSTITUTE({!$Record.Funding_Value__c}, &quot;,&quot;, &quot;.&quot;))</expression>
    </formulas>
    <formulas>
        <description>NTF Amount with minus pcoket data</description>
        <name>funcN_ntfamount</name>
        <dataType>Number</dataType>
        <expression>{!Get_NTF_Amount.ntf_amount__c} - {!varN_SumPocketFee}</expression>
        <scale>2</scale>
    </formulas>
