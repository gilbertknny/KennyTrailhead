    <decisions>
        <description>Check which user : CA Cabang or SPV AO or SPV BRO</description>
        <name>Check_Which_User</name>
        <label>Check Which User</label>
        <locationX>660</locationX>
        <locationY>1106</locationY>
        <defaultConnector>
            <targetReference>Loop_Child_Roles</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>NOT ONE OF THEM</defaultConnectorLabel>
        <rules>
            <name>SPV_AO</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>LowerCaseRoleName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>spv ao</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_User_ID_to_UserIDSPVAO</targetReference>
            </connector>
            <label>SPV AO</label>
        </rules>
        <rules>
            <name>SPV_BRO</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>LowerCaseRoleName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
