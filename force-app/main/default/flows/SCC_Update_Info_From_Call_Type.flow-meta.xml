    <formulas>
        <name>Casecategoryformulavariable</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK(TEXT({!callTypeObject.SSC_Case_Category__c}))), TEXT({!callTypeObject.SSC_Case_Category__c}), IF(NOT(ISBLANK(TEXT({!$Record.SCC_Case_Category__c}))), TEXT({!$Record.SCC_Case_Category__c}), &apos;&apos;) )</expression>
    </formulas>
    <formulas>
        <name>Descriptionandadditionaldetail</name>
        <dataType>String</dataType>
        <expression>IF(
    AND(
        NOT(ISBLANK({!$Record.Description})),
        {!$Record.SCC_Call_Type__c} = {!$Record__Prior.SCC_Call_Type__c},
        ISNULL({!$Record__Prior.SCC_Call_Type__c})
    ),
    {!$Record.Description} &amp; &apos;, &apos; &amp; {!callTypeObject.SCC_Additional_Details__c},
    IF(
        AND(
            NOT(ISBLANK({!$Record.Description})),
            {!$Record.SCC_Call_Type__c} &lt;&gt; {!$Record__Prior.SCC_Call_Type__c}
        ),
        {!$Record.Description} &amp; &apos;, &apos; &amp; {!callTypeObject.SCC_Additional_Details__c},
        IF(
            NOT(ISBLANK({!$Record.Description})),
            {!$Record.Description},
            {!callTypeObject.SCC_Additional_Details__c}
        )
    )
)</expression>
    </formulas>
    <formulas>
        <name>Descriptionfinal</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.Description}),
   {!callTypeObject.SCC_Additional_Details__c},
   {!$Record.Description}
)</expression>
    </formulas>
    <formulas>
        <name>finalDescription</name>
        <dataType>String</dataType>
        <expression>{!$Record.Description} + &apos; &apos; + {!callTypeObject.SSC_Case_Description__c}</expression>
    </formulas>
