    <formulas>
        <name>func_AccountBankCheckResult</name>
        <dataType>String</dataType>
        <expression>IF(
   AND(
      NOT(ISBLANK({!ValidateBankAccount.Results.OutputAccountNumber})),
      NOT(ISBLANK({!ValidateBankAccount.Results.OutputAccountName}))
   ),
   {!ValidateBankAccount.Results.OutputAccountNumber} &amp; &quot; a.n. &quot; &amp; {!ValidateBankAccount.Results.OutputAccountName},
   &quot;&quot;
)</expression>
    </formulas>
    <formulas>
        <name>func_BankAccountName</name>
        <dataType>String</dataType>
        <expression>IF(
   NOT(ISBLANK({!func_AccountBankCheckResult})),
   {!func_AccountBankCheckResult},
   {!Bank_Account_Name}
)</expression>
    </formulas>
    <formulas>
        <name>func_Is_Funding_Name_Contains_Hold</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!Funding_Name_Split}, &quot;Hold&quot;)</expression>
    </formulas>
    <formulas>
        <name>funcB_IsEditFunding</name>
        <dataType>Boolean</dataType>
        <expression>NOT(ISBLANK({!Get_Funding.Id}))</expression>
    </formulas>
    <formulas>
        <name>funcB_IsFundingValueCurrencyDisabled</name>
        <dataType>Boolean</dataType>
        <expression>IF({!Funding_Name_Pocket} == &quot;Pocket - Angsuran&quot; || {!Funding_Name_Pocket} == &quot;Pocket - Angsuran Installment 1&quot; || {!Funding_Name_Pocket} == &quot;Hold Dana Angsuran&quot; || {!Funding_Name_Pocket} == &quot;Hold Dana Angsuran Installment 1&quot;, TRUE, FALSE)</expression>
    </formulas>
    <formulas>
        <name>funcN_DefaultFundingValueCurrency</name>
        <dataType>Currency</dataType>
        <expression>IF({!Funding_Name_Pocket} == &quot;Pocket - Angsuran Installment 1&quot; || {!Funding_Name_Pocket} == &quot;Hold Dana Angsuran Installment 1&quot;, {!Get_Operation.Application__r.Survey__r.Monthly_Installment__c}, 
IF({!Funding_Name_Pocket} == &quot;Pocket - Angsuran&quot; || {!Funding_Name_Pocket} == &quot;Hold Dana Angsuran&quot;, IF(ISBLANK({!Funding_Value_Number}), 0, {!Funding_Value_Number}) * {!Get_Operation.Application__r.Survey__r.Monthly_Installment__c}, {!var_NewFunding.Funding_Value_Currency__c}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>funcN_DefaultFundingValueNumber</name>
        <dataType>Number</dataType>
        <expression>IF({!Funding_Name_Pocket} == &quot;Pocket - Angsuran&quot; || {!Funding_Name_Pocket} == &quot;Hold Dana Angsuran&quot;, {!var_NewFunding.Funding_Value_Currency__c}/{!var_NewFunding.Operation__r.Application__r.Survey__r.Monthly_Installment__c}, null)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>funcS_FundingType</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!var_NewFunding.Funding_Type__c}, &quot;Split&quot;) == TRUE, &quot;Split Funding&quot;, 
IF(ISPICKVAL({!var_NewFunding.Funding_Type__c}, &quot;Pocket&quot;) == TRUE, &quot;Pocket Funding&quot;, 
IF(ISPICKVAL({!var_NewFunding.Funding_Type__c}, &quot;Non Funding&quot;) == TRUE, &quot;Non Funding&quot;, TEXT({!var_NewFunding.Funding_Type__c}))))</expression>
    </formulas>
    <formulas>
        <name>funcS_SelectedFundingId</name>
        <dataType>String</dataType>
        <expression>{!var_SelectedNonFunding}&amp;{!var_SelectedPocket}&amp;{!var_SelectedSplit}</expression>
    </formulas>
    <formulas>
        <name>funcS_SelectedFundingName</name>
        <dataType>String</dataType>
        <expression>{!Funding_Name_Split}&amp;{!Funding_Name_Pocket}&amp;{!Funding_Name_NonFunding}</expression>
    </formulas>
    <formulas>
        <name>funcS_SelectedFundingType</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!Funding_Type}, &quot;Split&quot;), &quot;Split&quot;, 
IF(CONTAINS({!Funding_Type}, &quot;Pocket&quot;), &quot;Pocket&quot;, 
IF(CONTAINS({!Funding_Type}, &quot;Non Funding&quot;), &quot;Non Funding&quot;, {!Funding_Type})))</expression>
    </formulas>
    <formulas>
        <name>NotIsChanged</name>
        <dataType>Boolean</dataType>
        <expression>NOT({!Get_Funding.Operation__r.Loan_Information__r.Account.Is_Changed__c})</expression>
    </formulas>
