    <formulas>
        <name>formulaBirthdateForLoanInput</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!Get_Lead.Date_of_Birth__c})) &amp; &quot;-&quot; &amp;
IF(MONTH({!Get_Lead.Date_of_Birth__c}) &lt; 10, &quot;0&quot;, &quot;&quot;) &amp; TEXT(MONTH({!Get_Lead.Date_of_Birth__c})) &amp; &quot;-&quot; &amp;
IF(DAY({!Get_Lead.Date_of_Birth__c}) &lt; 10, &quot;0&quot;, &quot;&quot;) &amp; TEXT(DAY({!Get_Lead.Date_of_Birth__c}))</expression>
    </formulas>
    <formulas>
        <name>formulaBirthday</name>
        <dataType>String</dataType>
        <expression>TEXT(DAY({!Get_Lead.Date_of_Birth__c})) &amp; &quot;-&quot; &amp; 
TEXT(MONTH({!Get_Lead.Date_of_Birth__c})) &amp; &quot;-&quot; &amp; 
TEXT(YEAR({!Get_Lead.Date_of_Birth__c}))</expression>
    </formulas>
    <formulas>
        <name>formulaCustomerType</name>
        <dataType>String</dataType>
        <expression>IF({!var_CustomerType} = &quot;RO PBF&quot;, &quot;RO&quot;, &quot;NEW&quot;)</expression>
    </formulas>
