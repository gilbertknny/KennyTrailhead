<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>if_Nama_Pelapor_is_blank_on_case_if_account_is_added</name>
        <label>if Nama Pelapor is blank on case if account is added</label>
        <locationX>182</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Nama_Pelapor_is_blank</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.AccountId</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_account</targetReference>
            </connector>
            <label>Nama Pelapor is blank</label>
        </rules>
    </decisions>
    <description>The purpose of this flow is to update Nama Pelapor field from account name when creating case from Account</description>
    <environments>Default</environments>
    <interviewLabel>Auto_Polulate_Nama_Pelapor_from_Account {!$Flow.CurrentDateTime}</interviewLabel>
