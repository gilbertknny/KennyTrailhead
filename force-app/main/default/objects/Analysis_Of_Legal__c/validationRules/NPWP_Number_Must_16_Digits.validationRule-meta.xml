<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NPWP_Number_Must_16_Digits</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(npwp__c)),
    NOT(REGEX(npwp__c, &quot;^[0-9]{16}$&quot;))
)</errorConditionFormula>
    <errorDisplayField>npwp__c</errorDisplayField>
    <errorMessage>Please input 16 digit number only.</errorMessage>
</ValidationRule>
