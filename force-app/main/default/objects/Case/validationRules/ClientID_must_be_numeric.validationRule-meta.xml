<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ClientID_must_be_numeric</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Client_ID__c)), NOT(ISNUMBER(Client_ID__c)) )</errorConditionFormula>
    <errorMessage>ClientID harus numerik</errorMessage>
</ValidationRule>
