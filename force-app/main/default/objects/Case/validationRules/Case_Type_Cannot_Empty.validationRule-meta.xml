<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Type_Cannot_Empty</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Status, &quot;Closed&quot;),
    ISBLANK(  SCC_Call_Type__c  ),
!$Permission.By_Pass_Validation_for_Migration
)</errorConditionFormula>
    <errorMessage>Mohon isi Case Type</errorMessage>
</ValidationRule>
