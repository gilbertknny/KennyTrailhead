<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Change_Sub_Level_Eskalasi</fullName>
    <active>false</active>
    <description>[Support]</description>
    <errorConditionFormula>AND(
    ISCHANGED(SCC_Sub_Escalation_Level__c),
    TEXT(Status) != &apos;Escalated&apos;
)</errorConditionFormula>
    <errorMessage>User hanya dapat mengubah field &apos;Sub Level Eskalasi&apos; ketika status berada pada &apos;Waiting Document&apos;</errorMessage>
</ValidationRule>
