<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SCC_Call_Type_Mandatory</fullName>
    <active>true</active>
    <description>Call Type Mandatory for Case Status Except New/Cancelled.</description>
    <errorConditionFormula>AND( NOT($Permission.By_Pass_Validation_for_Migration) ,OR((TEXT(Status) = &apos;Working&apos;) ,(TEXT(Status) = &apos;Waiting Document&apos;),(TEXT(Status) = &apos;Disconnected&apos;),
(TEXT(Status) = &apos;Sub-status Assigned&apos;),(TEXT(Status) = &apos;Return&apos;),(TEXT(Status) = &apos;Closed&apos;),(TEXT(Status) = &apos;Escalated&apos;)) , ISBLANK(SCC_Call_Type__c))</errorConditionFormula>
    <errorDisplayField>SCC_Call_Type__c</errorDisplayField>
    <errorMessage>Pada saat status ini Call Type diwajibkan untuk diisi !</errorMessage>
</ValidationRule>
