<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Origin_Validation</fullName>
    <active>true</active>
    <description>Case origin cannot be updated 

update : without mms</description>
    <errorConditionFormula>AND(
    ISCHANGED(  Origin ),
    NOT(ISNEW()),
    TEXT(Origin) &lt;&gt; &quot;MMS&quot;
)</errorConditionFormula>
    <errorDisplayField>Origin</errorDisplayField>
    <errorMessage>Anda tidak dapat mengubah origin kasus.</errorMessage>
</ValidationRule>
