<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone2_Min_6_Digits</fullName>
    <active>false</active>
    <errorConditionFormula>IF(
OR(
ISBLANK(Mobile_Phone_2__c),
Mobile_Phone_2__c = &quot;&quot;
),
FALSE,
LEN(Mobile_Phone_2__c) &lt; 6
)</errorConditionFormula>
    <errorDisplayField>Mobile_Phone_2__c</errorDisplayField>
    <errorMessage>Please enter at least 6 digits</errorMessage>
</ValidationRule>
