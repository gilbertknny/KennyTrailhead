<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Admin_Validation_Phone</fullName>
    <active>false</active>
    <errorConditionFormula>(ISCHANGED(Phone) &amp;&amp;  $Profile.Name = &apos;System Administrator&apos;) || (ISCHANGED(Phone) &amp;&amp;  $Profile.Name != &apos;System Administrator&apos;) &amp;&amp; !ISBLANK(TEXT(SCC_Jenis_Nasabah__c))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>Anda tidak boleh merubah nomor telepon</errorMessage>
</ValidationRule>
