<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Place_of_Established_Must_Alphabet</fullName>
    <active>false</active>
    <errorConditionFormula>IF( $User.Is_Debug_User__c = TRUE,FALSE,AND( RecordType.DeveloperName = &quot;Company&quot;,OR(NOT(REGEX( Place_of_Established__c , &quot;^[a-zA-Z\\s]{3,80}$&quot;)))))</errorConditionFormula>
    <errorDisplayField>Place_of_Established__c</errorDisplayField>
    <errorMessage>Please input 3-80 character, alphabet only.</errorMessage>
</ValidationRule>
