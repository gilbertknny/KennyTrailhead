<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NPWP_Number_Must_15_or_16_Digits</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(NPWP_Number__c)),
NOT(REGEX(NPWP_Number__c, &quot;^[0-9]{15,16}$&quot;))
)</errorConditionFormula>
    <errorDisplayField>NPWP_Number__c</errorDisplayField>
    <errorMessage>Please input 15 or 16 digit number, numeric only</errorMessage>
</ValidationRule>
