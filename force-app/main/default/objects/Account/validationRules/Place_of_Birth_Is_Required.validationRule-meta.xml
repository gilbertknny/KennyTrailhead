<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Place_of_Birth_Is_Required</fullName>
    <active>false</active>
    <description>Optional for every applications</description>
    <errorConditionFormula>AND(
    RecordType.DeveloperName = &quot;Personal&quot;,
    ISBLANK(Place_of_Birth__c)
)</errorConditionFormula>
    <errorDisplayField>Place_of_Birth__c</errorDisplayField>
    <errorMessage>Complete this field</errorMessage>
</ValidationRule>
