<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_Name_Must_Alphabet</fullName>
    <active>false</active>
    <errorConditionFormula>IF( $User.Is_Debug_User__c = TRUE,FALSE,OR(NOT(REGEX(   Name , &quot;^[a-zA-Z ]*$&quot;))))</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Please input alphabet only.</errorMessage>
</ValidationRule>
