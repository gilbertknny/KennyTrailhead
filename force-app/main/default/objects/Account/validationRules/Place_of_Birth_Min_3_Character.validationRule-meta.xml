<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Place_of_Birth_Min_3_Character</fullName>
    <active>false</active>
    <description>Optional</description>
    <errorConditionFormula>IF(
    ISBLANK(Place_of_Birth__c),
    FALSE,
    LEN(Place_of_Birth__c) &lt; 3
)</errorConditionFormula>
    <errorDisplayField>Place_of_Birth__c</errorDisplayField>
    <errorMessage>Please enter at least 3 character</errorMessage>
</ValidationRule>
