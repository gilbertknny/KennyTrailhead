<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Married_Date_Is_Required</fullName>
    <active>false</active>
    <errorConditionFormula>IF( $User.Is_Debug_User__c = TRUE, FALSE, AND( Marital_Status__c  &lt;&gt; &quot;Married&quot;, ISBLANK( Married_Date__c )))</errorConditionFormula>
    <errorDisplayField>Married_Date__c</errorDisplayField>
    <errorMessage>Complete this field</errorMessage>
</ValidationRule>
