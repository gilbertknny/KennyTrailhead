<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Spouse_Place_of_Birth_Must_Alphabet</fullName>
    <active>false</active>
    <errorConditionFormula>(IF( $User.Is_Debug_User__c = TRUE,FALSE,OR(NOT(REGEX( Place_of_Birth__c , &quot;^[a-zA-Z ]*$&quot;)))))&amp;&amp; !IsVRBypassed__c</errorConditionFormula>
    <errorDisplayField>Place_of_Birth__c</errorDisplayField>
    <errorMessage>Number and special character are not allowed, for example / \ :*?&quot;&lt;&gt;|</errorMessage>
</ValidationRule>
