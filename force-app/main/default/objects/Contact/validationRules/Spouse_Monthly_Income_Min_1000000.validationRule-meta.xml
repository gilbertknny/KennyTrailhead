<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Spouse_Monthly_Income_Min_1000000</fullName>
    <active>false</active>
    <errorConditionFormula>OR(ISBLANK(Monthly_Income__c), Monthly_Income__c &lt; 1000000)</errorConditionFormula>
    <errorDisplayField>Monthly_Income__c</errorDisplayField>
    <errorMessage>Monthly income must be greater than Rp. 1.000.000</errorMessage>
</ValidationRule>
