<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SP_Name_Min_3_Characters</fullName>
    <active>false</active>
    <description>Spouse name should have at least 3 characters</description>
    <errorConditionFormula>(IF( $User.Is_Debug_User__c = TRUE,FALSE,IF(ISBLANK(Spouse_Name__c), FALSE, LEN( Spouse_Name__c) &lt; 3)))&amp;&amp; !IsVRBypassed__c</errorConditionFormula>
    <errorDisplayField>Spouse_Name__c</errorDisplayField>
    <errorMessage>Complete this field</errorMessage>
</ValidationRule>
