<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Locking_Form_Penilaian</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  NOT(ISNEW()),
  NOT(ISBLANK(PRIORVALUE(Luas_Tanah__c)))
) &amp;&amp; !IsVRByPassed__c</errorConditionFormula>
    <errorMessage>Form Penilaian Sudah di Submit, tidak bisa di ubah lagi.</errorMessage>
</ValidationRule>
