<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Min_6_Digits</fullName>
    <active>true</active>
    <errorConditionFormula>IF(OR(ISBLANK(Phone), ISNULL(Phone),Phone= &quot;&quot;),FALSE, LEN(Phone) &lt; 6)</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>Please enter at least 6 digits</errorMessage>
</ValidationRule>
