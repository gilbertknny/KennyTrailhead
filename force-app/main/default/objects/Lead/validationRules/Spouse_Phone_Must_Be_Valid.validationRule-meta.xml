<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Spouse_Phone_Must_Be_Valid</fullName>
    <active>true</active>
    <description>Phone 1 number length should be minimal 12 and in digits</description>
    <errorConditionFormula>IF(OR(ISBLANK(Spouse_Phone_1__c ), ISNULL(Spouse_Phone_1__c ),Spouse_Phone_1__c = &quot;&quot;),FALSE, NOT(REGEX(  Spouse_Phone_1__c , &quot;^(\\+?0)(?!0{2,})\\d*$&quot;)))</errorConditionFormula>
    <errorDisplayField>Spouse_Phone_1__c</errorDisplayField>
    <errorMessage>Please input correct phone number</errorMessage>
</ValidationRule>
