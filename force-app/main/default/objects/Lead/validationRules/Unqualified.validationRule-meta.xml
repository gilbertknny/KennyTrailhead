<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unqualified</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISCHANGED(Status),
    PRIORVALUE(Status) = &quot;Unqualified&quot;
)</errorConditionFormula>
    <errorMessage>Lead Degnan Status Unqualified Tidak Boleh Diperbarui</errorMessage>
</ValidationRule>
