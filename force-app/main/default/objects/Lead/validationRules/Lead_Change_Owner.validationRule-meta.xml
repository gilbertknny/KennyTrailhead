<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_Change_Owner</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISCHANGED(OwnerId),
    TEXT($User.Occupation__c) = &quot;Staff&quot;
)</errorConditionFormula>
    <errorMessage>Staff Tidak Bisa Change Owner</errorMessage>
</ValidationRule>
