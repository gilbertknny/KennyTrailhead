<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Branch_Not_In_ZIP_Code</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISNULL(Zip__c)) &amp;&amp; ISCHANGED(Branch__c) &amp;&amp;   NOT(ISBLANK(TEXT(Branch__c))) &amp;&amp; Zip__r.Branch_ID__c !=  TEXT(Branch__c)</errorConditionFormula>
    <errorMessage>Branch ID not match with ZIP Information</errorMessage>
</ValidationRule>
