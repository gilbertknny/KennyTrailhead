<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Spouse_Phone_Min_6_Digits</fullName>
    <active>true</active>
    <description>Phone 1 number length should be minimal 12 and in digits</description>
    <errorConditionFormula>IF(OR(ISBLANK(Spouse_Phone_1__c ), ISNULL(Spouse_Phone_1__c ),Spouse_Phone_1__c = &quot;&quot;),FALSE, LEN(Spouse_Phone_1__c) &lt; 6)</errorConditionFormula>
    <errorDisplayField>Spouse_Phone_1__c</errorDisplayField>
    <errorMessage>Please enter at least 6 digits</errorMessage>
</ValidationRule>
