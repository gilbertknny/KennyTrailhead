<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sertipikat_Number_Min_3_Digits</fullName>
    <active>false</active>
    <errorConditionFormula>IF( $User.Is_Debug_User__c = TRUE,FALSE,NOT(REGEX(  Sertifikat_Number__c , &quot;^[0-9]{3,}$&quot;)))</errorConditionFormula>
    <errorDisplayField>Sertifikat_Number__c</errorDisplayField>
    <errorMessage>Please enter at least 3 digit number</errorMessage>
</ValidationRule>
