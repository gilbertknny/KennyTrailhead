<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Asset_RT_Must_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(  Asset_Address_is_same_as__c , &quot;&quot;) ,NOT(REGEX( Asset_RT__c , &quot;^[0-9]*$$&quot;)))</errorConditionFormula>
    <errorDisplayField>Asset_RT__c</errorDisplayField>
    <errorMessage>Alphabet and Special character are not allowed, for example / \ :*?&quot;&lt;&gt;|</errorMessage>
</ValidationRule>
