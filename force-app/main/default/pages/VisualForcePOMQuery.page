<apex:page controller="VisualForcePOMQueryParamsController"> 
    <apex:pageBlock title="Outbound Call Information"> 
        <style type="text/css"> 
            .zui-table { 
                border: solid 1px #DDEEEE; 
                border-collapse: collapse; 
                border-spacing: 0; 
                font: normal 13px Arial, sans-serif; 
            } 
            .zui-table thead th { 
                background-color: #DDEFEF; 
                border: solid 1px #DDEEEE; 
                color: #336B6B; 
                padding: 10px; 
                text-align: left; 
                text-shadow: 1px 1px 1px #fff; 
            } 
            .zui-table tbody td { 
                border: solid 1px #DDEEEE; 
                color: #333;
                padding: 10px; 
                text-shadow: 1px 1px 1px #fff; 
            } 
        </style> 
        <table class="zui-table"> 
           <tr style="background-color: #DDEFEF;"> 
               <td>Parameter Name</td> 
               <td>Parameter Value</td> 
           </tr> 
            <tr> 
               <td>Campaign Name</td> 
               <td>{!campaign }</td> 
            </tr> 
            <tr> 
                <td>Customer</td> 
                <td>{!dialedNumber}</td> 
            </tr> 
            <tr>  
                <td>First Name</td> 
                <td>{!firstName }</td> 
            </tr> 
            <tr> 
                <td>Last Name</td> 
                <td>{!lastName }</td> 
            </tr> 
            <tr> 
                <td>Phone</td> 
                <td>{!phone }</td> 
            </tr> 
            <tr> 
                <td>Time Zone</td> 
                <td>{!timeZone }</td> 
             </tr> 
             <tr> 
                <td>E-mail</td> 
                <td>{!email }</td> 
             </tr> 
             <tr> 
                <td>Zip Code</td> 
                <td>{!zipCode }</td> 
              </tr> 
              <tr> 
                <td>DNIS</td> 
                <td>{!dnis }</td> 
              </tr> 
         </table> 
 
    </apex:pageBlock>
</apex:page>