                </ui:scrollerWrapper>
            </lightning:accordionSection>
            <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c != '8713'}">
            <lightning:accordionSection label="EJ" name="H">
                <div class="slds-clearfix slds-m-around_small">
                    <div class="slds-clearfix">
                        <div class="slds-float_left">
                            <lightning:button variant="destructive" label="Delete" onclick="{! c.deletedEJ }" disabled="{!v.case.IsLocked__c || v.noSelectionMade}"/>
                        </div>
                        <div class="slds-float_left slds-m-left_small">
                            <lightning:button variant="destructive" label="Delete All" onclick="{! c.deletedAllEJ }" disabled="{!v.case.IsLocked__c || v.EJcolumns.length == 0}"/>
                        </div>
                        <div class="slds-float_right slds-m-right_small">
                            <lightning:button variant="destructive" label="Highlight EJ" onclick="{! c.highlightEJ }" disabled="{!v.case.IsLocked__c || v.noSelectionMade}"/>
                        </div>
                    </div>
                </div>
                <ui:scrollerWrapper class="scrollerSize">
                    <!-- data table start-->  
                    <table class="slds-table slds-table_bordered slds-table_cell-buffer">
                        <thead>
                            <tr class="slds-text-title_caps">
                            <!--header checkbox for select all-->
                                <th scope="col">
                                    <div class="slds-truncate" title="SELECT">SELECT</div>
                                </th>
                                <th scope="col">
                                    <div class="slds-truncate" title="EJ Log">EJ LOG</div>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <aura:iteration items="{!v.EJcolumns}" var="obj">
                                <tr class="{!obj.Highlight__c ? 'yellow2-row' : ''}">
                                    <th scope="row">
                                        <div class="slds-truncate" title="{!obj.IsDeleted__c}">
                                            <label class="slds-checkbox">
                                                <ui:inputCheckbox aura:id="checkbox" text="{!obj.Id}"  value="{!obj.IsDeleted__c}" change="{!c.selectEJ}"/>
                                                <span class="slds-checkbox_faux"></span>
                                                <span class="slds-form-element__label text"></span>
                                            </label>
                                        </div>
                                    </th>
                                    <th scope="row" class="{!'some-class left-align'}">
                                        <div class="slds-truncate" title="{!obj.EJ_Log__c}" style="white-space: pre-wrap;">
                                            {!obj.EJ_Log__c}
                                        </div>
                                    </th>
                                </tr>
                            </aura:iteration>
                        </tbody>
                    </table>
                    <!-- DataTable End -->
                </ui:scrollerWrapper>
            </lightning:accordionSection>
            </aura:if>
        </lightning:accordion>

    </div>
</aura:component>