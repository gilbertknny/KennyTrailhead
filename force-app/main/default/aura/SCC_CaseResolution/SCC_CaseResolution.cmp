<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:lightningQuickAction,lightning:actionOverride,lightning:hasPageReference" 
                access="global" controller="SCC_CaseResolution_UI">
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="activeSections" type="List" default="['J','A','B','C','D','E','G','H','I']" />
    <aura:attribute name="attdata" type="Object"/>
    <aura:attribute name="attcolumns" type="List"/>
    <aura:attribute name="quetrndata" type="Object"/> 
    <aura:attribute name="mutrekdata" type="Object"/>
    <aura:attribute name="srcmutrekdata" type="Object"/>
    <aura:attribute name="srcmutrekcolumns" type="List"/>
    <aura:attribute name="rekondata" type="Object"/>
    <aura:attribute name="cuspro" type="Object"/>
    <aura:attribute name="srcrekondata" type="Object"/>
    <aura:attribute name="srcrekoncolumns" type="List"/>
    <aura:attribute name="aktkardata" type="Object"/>
    <aura:attribute name="aktkarcolumns" type="List"/>
    <aura:attribute name="srcaktkardata" type="Object"/>
    <aura:attribute name="srcaktkarcolumns" type="List"/>
    <aura:handler name="init" value="{! this }" action="{! c.init }"/>
    <aura:attribute name="fileName" type="String" />
    <aura:attribute name="filetype" type="List" default="['.pdf', '.doc', '.docx', '.jpg', '.jpeg', '.png', '.zip', '.rar', '.txt']" />
    <aura:attribute name="caseObj" type="Object"/>
    <aura:attribute name="calltypeObj" type="Object" default=""/>
    <aura:attribute name="tidObj" type="Object" default=""/>
    <aura:attribute name="fituridObj" type="Object" default=""/>
    <aura:attribute name="case" type="Case" default="{ 'sobjectType': 'Case' }"/>
    <aura:attribute name="recordLoadError" type="String"/>
    <force:recordData aura:id="recordLoader"
                      recordId="{!v.recordId}"
                      fields="Id,SCC_Call_Type__c,Terminal_ID__c,AccountId,SCC_Account_Number__c,SCC_Transaction_Date__c,SCC_Amount__c,SCC_Call_Type_Feature__c,SCC_Card_Number__c,ContactId,OwnerId"
                      targetFields="{!v.caseObj}"
                      targetError="{!v.recordLoadError}"/>
    <aura:attribute name="call_type" type="String"/>
    <aura:attribute name="Unit_Kerja" type="Object" default=""/>
    <aura:attribute name="fiturid" type="String"/>
    <aura:attribute name="tid" type="String"/>
    <aura:attribute name="acc_num_dest" type="String"/>
    <aura:attribute name="kol_rmk" type="String"/>
    <aura:attribute name="trx_dt" type="Date" />
    <aura:attribute name="tgl_trx_akar" type="Date" />
    <aura:attribute name="filrec" type="String" default=""/>
    <aura:attribute name="optionsWA" type="List" default="[
    {'label': 'Normal/Sukses', 'value': 'Berhasil'},
    {'label': 'Gagal', 'value': 'Gagal'},
    {'label': 'Tidak Kirim Notifikasi', 'value': '-'}
    ]"/>
    <aura:attribute name="optionsWA2" type="List" default="[
    {'label': 'Normal/Sukses', 'value': 'Berhasil'},
    {'label': 'Gagal', 'value': 'Gagal'},
    {'label': 'Card Replace', 'value': 'Card Replace'},
    {'label': 'Tidak Kirim Notifikasi', 'value': '-'}
    ]"/>
    <aura:attribute name="valwa" type="String" default=""/>
    <aura:attribute name="isiwa" type="String" default=""/>
    <aura:attribute name="pageSize" type="Integer" default="100" description="number of records to be display on per page"/>
    <!-- rekening koran -->
    <aura:attribute name="rekoncolumns" type="List"/>
    <aura:attribute name="tgl_awal" type="Date" />
    <aura:attribute name="tgl_akhir" type="Date" />
    <aura:attribute name="startPageRekeningKoran" type="Integer" />
    <aura:attribute name="endPageRekeningKoran" type="Integer"/>
    <aura:attribute name="selectedRekeningKoran" type="integer" default="0" description="selected Records Count"/>
    <aura:attribute name="totalRekeningKoran" type="Integer"/>
    <aura:attribute name="currentPageRekeningKoran" type="integer" default="1"/>
    <aura:attribute name="totalPagesRekeningKoran" type="integer"/>
    <aura:attribute name="PaginationRekeningKoran" type="list"/>
    <aura:attribute name="listRekeningKoran" type="list"/>
    <aura:attribute name="RekonSelected" type="list"/>
    <aura:attribute name="isSCC_BO_NFT_ISSUER" type="Boolean" default="false"/>
    <aura:attribute name="isCase8410" type="Boolean" default="false"/>
    <aura:attribute name="isCase8412" type="Boolean" default="false"/>
    <aura:attribute name="isSCC_BO_Supervisor_NFT_ISSUER" type="Boolean" default="false"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!-- rekon -->
    <aura:attribute name="mutrekcolumns" type="List"/>
    <aura:attribute name="tgl_trx_rekon" type="Date" />
    <aura:attribute name="startPageRekon" type="Integer" />
    <aura:attribute name="endPageRekon" type="Integer"/>
    <aura:attribute name="selectedRekon" type="integer" default="0" description="selected Records Count"/>
    <aura:attribute name="totalRekon" type="Integer"/>
    <aura:attribute name="currentPageRekon" type="integer" default="1"/>
    <aura:attribute name="totalPagesRekon" type="integer"/>
    <aura:attribute name="PaginationRekon" type="list"/> 
    <aura:attribute name="listRekon" type="list"/> 
    <!-- Berita Acara Opname  -->
    <aura:attribute name="baocolumns" type="List"/>
    <aura:attribute name="tgl_trx_bao" type="Date" />
    <aura:attribute name="startPageBAO" type="Integer" />
    <aura:attribute name="endPageBAO" type="Integer"/>
    <aura:attribute name="selectedBAO" type="integer" default="0" description="selected Records Count"/>
    <aura:attribute name="totalBAO" type="Integer"/>
    <aura:attribute name="currentPageBAO" type="integer" default="1"/>
    <aura:attribute name="totalPagesBAO" type="integer"/>
    <aura:attribute name="PaginationBAO" type="list"/> 
    <aura:attribute name="listBAO" type="list"/> 
     <!-- AktivtasKartu -->
     <aura:attribute name="noSelectionKartu" type="Boolean" default="true"/>
     <aura:attribute name="Aktivitaskartucolumns" type="List"/>
     <aura:attribute name="tgl_trx_kartu" type="Date" />
     <aura:attribute name="time_trx_kartu" type="Time" />
     <aura:attribute name="startPageKartu" type="Integer" />
     <aura:attribute name="endPageKartu" type="Integer"/>
     <aura:attribute name="selectedKartu" type="integer" default="0" description="selected Records Count"/>
     <aura:attribute name="totalAktivitasKartu" type="Integer"/>
     <aura:attribute name="currentPageKartu" type="integer" default="1"/>
     <aura:attribute name="totalPagesKartu" type="integer"/>
     <aura:attribute name="PaginationKartu" type="list"/> 
     <aura:attribute name="listAktivitasKartu" type="list"/>  
     <aura:attribute name="fiturName" type="String" />
     <aura:attribute name="nomorFitur" type="String" />
     
     <!-- Brizzi Card Inquiry -->
     <!-- <aura:attribute name="noSelectionKartu" type="Boolean" default="true"/> -->
     <aura:attribute name="isButtonHistory" type="Boolean" default="true"/>
     <aura:attribute name="isButtonPartnerHistory" type="Boolean" default="true"/>
     <aura:attribute name="DataKartu" type="list"/>
     <aura:attribute name="kartuBrizzi" type="String" />

     <!-- Brizzi Trx History -->
     <aura:attribute name="startPageTrxHistory" type="Integer" />
     <aura:attribute name="endPageTrxHistory" type="Integer"/>
     <aura:attribute name="totalTrxHistory" type="Integer"/>
     <aura:attribute name="currentPageTrxHistory" type="integer" default="1"/>
     <aura:attribute name="totalPagesTrxHistory" type="integer"/>
     <aura:attribute name="DataTrxHistory" type="list"/> 
     <aura:attribute name="listTrxHistory" type="list"/>
     <aura:attribute name="countTrxHistory" type="Boolean" default="true"/>

     <!-- Brizzi Partner Trx History -->
     <aura:attribute name="startPagePtrTrxHistory" type="Integer" />
     <aura:attribute name="endPagePtrTrxHistory" type="Integer"/>
     <aura:attribute name="totalPtrTrxHistory" type="Integer"/>
     <aura:attribute name="currentPagePtrTrxHistory" type="integer" default="1"/>
     <aura:attribute name="totalPagesPtrTrxHistory" type="integer"/>
     <aura:attribute name="DataPtrTrxHistory" type="list"/> 
     <aura:attribute name="listPtrTrxHistory" type="list"/>
     <aura:attribute name="countPtrTrxHistory" type="Boolean" default="true"/>

     <aura:attribute name="channelTopupOptions" type="List" default="[]"/>

     <!-- EJ -->
     <aura:attribute name="processing" type="Boolean" default="false" />
     <aura:attribute name="dateRequest" type="String" />
     <aura:attribute name="buttonType" type="String" />
     <aura:attribute name="isProcessing" type="Boolean" default="false" />
     <aura:attribute name="noSelectionMade" type="Boolean" default="true"/>
     <aura:attribute name="selectedItems" type="List" default="[]" />
     <aura:attribute name="checkedCount" type="Integer" default="0" />
     <aura:attribute name="initialData" type="List" />  
     <aura:attribute name="filter" type="String" />  
     <aura:handler name="change" value="{!v.filter}" action="{!c.handleFilter}" />
     <aura:attribute name="originalEJData" type="List" />
     <aura:attribute name="originalEJTotal" type="Integer" />
     <aura:attribute name="isOpen" type="Boolean" default="false"/>
     <aura:attribute name="responseData" type="Object[]"/>
     <aura:attribute name="filteredData" type="Object[]"/>
     <aura:attribute name="searchKeyword" type="String"/>
     <aura:attribute name="Size" type="Integer" description="number of records to be display on per page"/>
     <!-- <aura:attribute name="activeTabId" type="String" default=""/> -->
     <aura:attribute name="tgl_input_awal" type="Date" />
     <aura:attribute name="tgl_input_akhir" type="Date" />
     <aura:attribute name="EJcolumns" type="List"/>
     <aura:attribute name="startPageEJ" type="Integer" />
     <aura:attribute name="endPageEJ" type="Integer"/>
     <aura:attribute name="selectedEJ" type="integer" default="0" description="selected Records Count"/>
     <aura:attribute name="totalEJ" type="Integer"/>
     <aura:attribute name="currentPageEJ" type="integer" default="1"/>
     <aura:attribute name="totalPagesEJ" type="integer"/>
     <aura:attribute name="PaginationEJ" type="list"/> 
     <aura:attribute name="listEJ" type="list" default="[]"/>
    <!-- Adjust Deposit -->
    <aura:attribute name="adjustmentColumns" type="List"/>
    <aura:attribute name="noSelectionAdjust" type="Boolean" default="true"/>
    <!-- Replace Card -->
    <aura:attribute name="replacecolumns" type="List"/>
    <aura:attribute name="noSelectionReplace" type="Boolean" default="true"/>
    <!-- Queuetrans -->
    <aura:attribute name="quetrncolumns" type="List"/>
    <aura:attribute name="loaded" type="Boolean" default="true" />
    <div class="slds-p-around_medium">
        <lightning:messages />
        <!--loading spinner start-->
        <aura:if isTrue="{!v.loaded}">
            <aura:set attribute="else">
                <div aura:id="spinnerId" class="slds-spinner_container">
                    <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                        <span class="slds-assistive-text">Loading...</span>
                        <div class="slds-spinner__dot-a"></div>
                        <div class="slds-spinner__dot-b"></div>
                    </div>
                </div>
            </aura:set>
        </aura:if>
        <!-- Loading spinner end-->
        <lightning:layout title="" multipleRows="true" verticalAlign="start">
            <lightning:layoutItem size="6"  padding="around-small">
                <div class="page-section page-right">
                    <aura:if isTrue="{!v.case.IsLocked__c}">
                        <lightning:input name="nm_nsbh" disabled="true" value="{!v.calltypeObj.label}" label="Case Type" />
                        <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c != '8713'}">
                        <lightning:input name="acc_num" disabled="true" value="{!v.tidObj.label}" label="TID" />
                        </aura:if>
                        <aura:set attribute="else">
                            <c:CustomLookup objectName="SSC_Call_Type__c" fieldName="SCC_Case_Type_Detail__c" label="Case Type" iconName="standard:account" selectedRecord="{!v.calltypeObj}" 
                                            placeholder="Search..." filterRecord="1" fieldRecord="ActiveString__c"/>
                            <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c != '8713'}">
                            <c:CustomLookupAPI label="TID" iconName="standard:account" selectedRecordId="{!v.tidObj}" trx_date="{!v.case.SCC_Transaction_Date__c}" placeholder="Search..."/>
                            </aura:if>
                        </aura:set>
                    </aura:if> 
                    <lightning:input name="nm_nsbh" disabled="true" value="{!v.case.Account.Name}" label="Nama Nasabah" />
                    <lightning:input name="acc_num" disabled="true" value="{!v.case.SCC_Account_Number__c}" label="Nomor Rekening" />
                    <lightning:input disabled="true" name="no_telp" label="No HP / Telepon Terkini" value="{!v.case.Cust_Current_Phone__c}"/> 
                    <!-- <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c == '8713'}">
                    <lightning:select 
                        name="channelTopup"
                        label="Channel Topup"
                        disabled="{!v.case.IsLocked__c}"
                        value="{!v.case.Channel_Topup__c}"
                        required="true"
                    >
                        <option text="Select" value="" />
                        <aura:iteration items="{!v.channelTopupOptions}" var="option">
                            <option text="{!option.label}" value="{!option.value}" />
                        </aura:iteration>
                    </lightning:select>
                    </aura:if> -->
                    <lightning:input type="datetime" name="trx_date" label="Tanggal Transaksi" value="{!v.case.SCC_Waktu_Transaksi__c}" disabled="{!v.case.IsLocked__c}"/>
                </div>
            </lightning:layoutItem>
            <lightning:layoutItem size="6"   padding="around-small">
                <div class="page-section page-right">
                    <aura:if isTrue="{!v.case.IsLocked__c}">
                        <lightning:input name="nm_nsbh" disabled="true" value="{!v.fituridObj.label}" label="Fiturid" />

                            <aura:if isTrue="{!v.isCase8410}">
                                <aura:if isTrue="{!v.isSCC_BO_NFT_ISSUER}">
                                    <lightning:input disabled="true" value="{!v.Unit_Kerja.label}" label="Unit Kerja" />
                                </aura:if>

                                <aura:if isTrue="{!v.isSCC_BO_Supervisor_NFT_ISSUER}">
                                    <lightning:input disabled="true" value="{!v.Unit_Kerja.label}" label="Unit Kerja" />
                                </aura:if>
                            </aura:if>

                            <aura:set attribute="else">
                                <c:CustomLookup objectName="SCC_Call_Type_Feature__c" fieldName="SCC_Fitur_Detail__c" label="Fiturid" iconName="standard:account" selectedRecord="{!v.fituridObj}" 
                                placeholder="Search..." filterRecord="{!V.calltypeObj.value}" fieldRecord="Call_Type__c"/>
                                
                                    <aura:if isTrue="{!v.isCase8410}">
                                        <aura:if isTrue="{!v.isSCC_BO_NFT_ISSUER}">
                                            <c:CustomLookup objectName="Branch_Unit__c" fieldName="Name" label="Unit Kerja" iconName="standard:account" selectedRecord="{!v.Unit_Kerja}" placeholder="Search..."/>
                                        </aura:if>

                                        <aura:if isTrue="{!v.isSCC_BO_Supervisor_NFT_ISSUER}">
                                            <c:CustomLookup objectName="Branch_Unit__c" fieldName="Name" label="Unit Kerja" iconName="standard:account" selectedRecord="{!v.Unit_Kerja}" placeholder="Search..."/>
                                        </aura:if>
                                    </aura:if>

                            </aura:set>
                    </aura:if> 
                    <lightning:input name="cd_num" disabled="true" value="{!v.case.SCC_Card_Number__c}" label="Nomor Kartu" />
                    <lightning:input name="nm_ddmst" disabled="true" value="{!v.cuspro.nama}" label="Nama DDMast" />
                    <lightning:input name="act_num_ddmst" disabled="true" value="{!v.cuspro.acc_num}" label="Acct Number DDMast" />
                    <lightning:input label="Account Number Dest" name="acc_num_dest" value="{!v.acc_num_dest}" disabled="true" />
                    <lightning:input label="Nomor BRIVA" name="SCC_Nomor_Briva_Tagihan__c" value="{!v.case.SCC_Nomor_Briva_Tagihan__c}" disabled="true" />
                    <lightning:input label="Nomor Brizzi" name="SCC_Brizzi_Card_Number__c" value="{!v.case.SCC_Brizzi_Card_Number__c}" disabled="true" />
                </div>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:layout>
            <lightning:layoutItem size="12"   padding="around-small">
                <lightning:input type="number" label="Nominal" name="nominal" value="{!v.case.SCC_Amount__c}" disabled="true" class="right-align" />
                <ui:inputTextArea aura:id="field" label="Detail Informasi"  value="{!v.case.Description}" rows="7" cols="20" disabled="true"/>
                <ui:inputTextArea aura:id="field" label="Remark"  value="{!v.case.SCC_Drone_Remark_Update__c}" rows="7" cols="20" disabled="true"/>
                <ui:inputTextArea aura:id="field" label="Kolom Remark"  value="{!v.kol_rmk}" rows="7" cols="20"/>
                <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c != '8713'}">
                    <lightning:radioGroup name="radioGroup" label="Jenis Notifikasi" options="{! v.optionsWA }" value="{! v.valwa }" type="radio" />
                <aura:set attribute="else">
                    <lightning:radioGroup name="radioGroup" label="Jenis Notifikasi" options="{! v.optionsWA2 }" value="{! v.valwa }" type="radio" />
                </aura:set>
                </aura:if>
                <ui:inputTextArea aura:id="field" label="Text Notifikasi"  value="{!v.isiwa}" rows="7" cols="20" disabled="true"/>
                <!-- <lightning:fileUpload label="Attach receipt" name="fileUploader" multiple="false" accept="{!v.filetype}" onuploadfinished="{!c.handleUploadFinished}" /> -->
                <lightning:button type="submit" name="Submit" label="Submit" class="slds-m-top_medium" onclick="{!c.setCase}"/>
            </lightning:layoutItem>
        </lightning:layout>
        <lightning:accordion activeSectionName="{!v.activeSections}" allowMultipleSectionsOpen="true" >
            <!--
            <lightning:accordionSection label="Attachment" name="A">
                <lightning:datatable data="{! v.attdata }" columns="{! v.attcolumns }" keyField="Id" hideCheckboxColumn="true"/> 
            </lightning:accordionSection>
            -->
            <lightning:accordionSection label="Data Pendukung" name="F">
                <lightning:tabset selectedTabId="one">
                    <aura:if isTrue="{!v.case.SCC_Case_Type_for_Suggested_Article__c == '8713'}">
                    <lightning:tab label="Brizzi" id="one">
                        <lightning:layout title="" multipleRows="true" verticalAlign="start">
                            <lightning:layoutItem size="12"  padding="around-small">
                                <div class="page-section page-right">
                                    <lightning:input aura:id="kartuBrizzi" name="kartu_brizzi" label="Kartu Brizzi" value="{!v.case.SCC_Brizzi_Card_Number__c}" required="true" disabled="true"/>
                                    <lightning:button type="submit" name="cari_kartuBrizzi" label="Cari" class="slds-m-top_medium" onclick="{!c.searchKartuBrizzi}"/>
                                </div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="12"  padding="around-small">
                                <div class="page-section page-right">
                                    <ui:scrollerWrapper class="scrollerSize">
                                        <!-- data table start-->  
                                        <table class="slds-table slds-table_bordered slds-table_cell-buffer">
                                            <thead>
                                                <tr class="slds-text-title_caps">
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Saldo">Status Kartu</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Saldo Deposit">Saldo</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Debet">Saldo Deposit</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Top Up">Akumulasi Debet</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Top Up Bulan ini">Akumulasi Kredit</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Kode Cabang">Akumulasi Top Up Bulan Ini</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Tipe Kartu">Last Trx Date</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Expired">Last Trx Amount</div>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <aura:iteration items="{!v.DataKartu}" var="obj">
                                                    <tr>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].status_kartu}">
                                                                {!obj.data[0].status_kartu}
                                                            </div>
                                                        </th> 
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].saldo}">
                                                                {!obj.data[0].saldo}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].saldo_topup}">
                                                                {!obj.data[0].saldo_topup}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].akum_debet}">
                                                                {!obj.data[0].akum_debet}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].akum_kredit}">
                                                                {!obj.data[0].akum_kredit}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].topup}">
                                                                {!obj.data[0].topup}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].last_trx_date}">
                                                                {!obj.data[0].last_trx_date}
                                                            </div>
                                                        </th>
                                                        <th scope="row">
                                                            <div class="slds-truncate" title="{!obj.data[0].last_trx_amount}">
                                                                {!obj.data[0].last_trx_amount}
                                                            </div>
                                                        </th>
                                                    </tr>
                                                </aura:iteration>
                                            </tbody>
                                        </table>
                                    </ui:scrollerWrapper>
                                </div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="12"  padding="around-small">
                                <div class="page-section page-right">
                                    <lightning:button type="submit" name="trx_history" label="Trx History" class="slds-m-top_medium" onclick="{!c.searchTrxHistory}" disabled="{!v.isButtonHistory}"/>
                                    <lightning:button type="submit" name="partnership_trx_history" label="Partnership Trx History" class="slds-m-top_medium" onclick="{!c.searchPtrTrxHistory}" disabled="{!v.isButtonPartnerHistory}"/>
                                </div>
                            </lightning:layoutItem>
                            <lightning:layoutItem size="12"  padding="around-small">
                                <p class="slds-m-around_small">
                                    <span>
                                        Trx History
                                    </span>
                                </p>
                                <div class="page-section page-right">
                                    <ui:scrollerWrapper class="scrollerSize">
                                        <!-- data table start-->  
                                        <table class="slds-table slds-table_bordered slds-table_cell-buffer">
                                            <thead>
                                                <tr class="slds-text-title_caps">
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Saldo">MID</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Saldo Deposit">TID</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Debet">Tgl Trx</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Top Up">Jam Trx</div>
                                                    </th>
                                                    <th scope="col">
                                                        <div class="slds-truncate" title="Akumulasi Top Up Bulan ini">Jenis Trx</div>
