<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingGuestRules>
        <fullName>Get_Master_Data_Round_Robin_for_Web_to_Lead</fullName>
        <accessLevel>Read</accessLevel>
        <description>Get master data record with record type Round Robin for Web to Lead round robin assignment</description>
        <label>Get Master Data Round Robin for Web to Lead</label>
        <sharedTo>
            <guestUser>Web_to_Lead</guestUser>
        </sharedTo>
        <booleanFilter>(1 OR 2 OR 4) AND 3</booleanFilter>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Round Robin Member</value>
        </criteriaItems>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Round Robin Member Account Matrix</value>
        </criteriaItems>
        <criteriaItems>
            <field>Name</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Master Branch</value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
    <sharingGuestRules>
        <fullName>Master_Data_Guest</fullName>
        <accessLevel>Read</accessLevel>
        <label>Master Data Guest</label>
        <sharedTo>
            <guestUser>Claim_Registration1</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>CreatedById</field>
            <operation>startsWith</operation>
            <value>005</value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
</SharingRules>
